[omniwatch.collectors]

#port = 8000
port = 8001
usermode = True
usermode_port = 8001
enable_rocm_smi = True
enable_slurm = True
corebinding = 31
logfile = exporter.log

[omniwatch.collectors.slurm]

host_skip = "login.*"
enable_annotations = True

[omniwatch.report]

[omniwatch.query]
prometheus_url = http://localhost:9090
system_name = HPC Fund

# options for user-spawned promserver
[omniwatch.promserver]
template = prometheus.yml.template
logfile = prom_server.log

# hpcfund example
binary = /home1/karl/downloads/prometheus-2.45.1.linux-amd64/prometheus
datadir = data_prom
corebinding = 95

